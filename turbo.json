{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*",
        "prisma/schema.prisma"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**",
        "generated/**"
      ],
      "env": [
        "NEXTAUTH_URL",
        "NEXTAUTH_SECRET",
        "JWT_SECRET_KEY",
        "AUTH_GOOGLE_ID",
        "AUTH_GOOGLE_SECRET",
        "GOOGLE_MAPS_API_KEY"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "start": {
      "dependsOn": [
        "build"
      ]
    }
  }
}