{
  "version": 2,
  "buildCommand": "cd ../.. && npx turbo run build --filter=@repo/api",
  "installCommand": "cd ../.. && npm install",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ]
}